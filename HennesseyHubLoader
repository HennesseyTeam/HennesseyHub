HennesseyHub.Name = "HennesseyHub"
HennesseyHub.ZIndexBehavior = Enum.ZIndexBehavior.Global
HennesseyHub.DisplayOrder = 9000000
if syn and syn.protect_gui and (not islclosure(syn.protect_gui)) then
syn.protect_gui(HennesseyHub)
HennesseyHub.Parent = game:GetService("CoreGui")
	warn("HennesseyHub Parent = syn.protect_gui")
elseif gethui then
HennesseyHub.Parent = gethui()
	warn("HennesseyHub Parent = gethui")
else
	HennesseyHub.Parent = game:GetService("CoreGui")
	warn("HennesseyHub Parent = CoreGui")
end

