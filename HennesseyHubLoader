	function OnClicked()
		wait(0.7)
		if game.PlaceId == 9217365103 then --Test Game 1
			print("1")
			
			game.StarterGui:SetCore("SendNotification", {
				Title    = "Hennessey Hub Alpha";
				Text     = "Test Game 1";
				Duration = 5;
			})
			
		elseif game.PlaceId == 9217366438 then --Test Game 2
			print("2")
			
