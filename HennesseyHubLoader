local HennesseyHub = Instance.new('ScreenGui')
HennesseyHub.Name = "HennesseyHub"
HennesseyHub.ZIndexBehavior = Enum.ZIndexBehavior.Sibling
HennesseyHub.Parent = game:GetService("Players").LocalPlayer:WaitForChild("PlayerGui")

local HennesseyHubFrame = Instance.new('Frame')
HennesseyHubFrame.Name = "HennesseyHubFrame"
HennesseyHubFrame.Position = UDim2.new(0.3017,0,0.2935,0)
HennesseyHubFrame.Size = UDim2.new(0,393,0,150)
HennesseyHubFrame.BackgroundColor3 = Color3.new(0.1765,0.1765,0.1765)
HennesseyHubFrame.Parent = HennesseyHub

local GameText = Instance.new('TextButton')
GameText.Name = "GameText"
GameText.Position = UDim2.new(0.2443,0,0.5,0)
GameText.Size = UDim2.new(0,200,0,50)
GameText.BackgroundColor3 = Color3.new(0.098,0.0431,0.0431)
GameText.Text = "Text"
GameText.TextColor3 = Color3.new(1,1,1)
GameText.Font = Enum.Font.Ubuntu
GameText.TextSize = 14
GameText.Parent = HennesseyHubFrame


pcall(function()
	local script = { Parent = GameText }
	function OnClicked() 
		if game.PlaceId == 9217365103 then
			print("1")
		elseif game.PlaceId == 9217366438 then
			print("2")
		else
			print("Universal ESP + Aimbot")
		end
	end 
	
	script.Parent.MouseButton1Down:connect(OnClicked)
	
end)


pcall(function()
	local script = { Parent = GameText }
	local gametext = "text"
	
	if game.PlaceId == 9217365103 then
		gametext = "Game 1"
	elseif game.PlaceId == 9217366438 then
		gametext = "Game 2"
	else
		gametext = "Universal ESP + Aimbot"
	end
	
	
	script.Parent.Text = gametext
end)


pcall(function()
	local script = { Parent = GameText }
	function OnClicked() 
		wait(0.1)
		script.Parent.Parent:Destroy()
	end 
	
	script.Parent.MouseButton1Down:connect(OnClicked)
end)

local Close = Instance.new('TextButton')
Close.Name = "Close"
Close.Position = UDim2.new(0.9389,0,0,0)
Close.Size = UDim2.new(0,24,0,25)
Close.BackgroundColor3 = Color3.new(0.098,0.0431,0.0431)
Close.Text = "X"
Close.TextColor3 = Color3.new(1,1,1)
Close.Font = Enum.Font.Ubuntu
Close.TextSize = 14
Close.Parent = HennesseyHubFrame


pcall(function()
	local script = { Parent = Close }
	function OnClicked() 
		script.Parent.Parent.Parent:Destroy()
	end 
	
	script.Parent.MouseButton1Down:connect(OnClicked)
end)

local ImageLabel = Instance.new('ImageLabel')
ImageLabel.Position = UDim2.new(0.4224,0,0.0667,0)
ImageLabel.Size = UDim2.new(0,60,0,53)
ImageLabel.BackgroundColor3 = Color3.new(1,1,1)
ImageLabel.BackgroundTransparency = 1
ImageLabel.Image = "rbxassetid://9217564693"
ImageLabel.Parent = HennesseyHubFrame


pcall(function()
	local script = { Parent = HennesseyHubFrame }
	local UIS = game:GetService("UserInputService")
	local dragSpeed = -math.huge
	
	local dragToggle = nil
	local dragInput = nil
	local dragStart = nil
	local dragPos = nil
	
	function dragify(Frame)
		function updateInput(input)
	        local Delta = input.Position - dragStart
	        local Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + Delta.X, startPos.Y.Scale, startPos.Y.Offset + Delta.Y)
	        script.Parent.Position = Position
		end
		
	    Frame.InputBegan:Connect(function(input)
	        if (input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch) and UIS:GetFocusedTextBox() == nil then
	            dragToggle = true
	            dragStart = input.Position
	            startPos = Frame.Position
	            input.Changed:Connect(function()
	                if input.UserInputState == Enum.UserInputState.End then
	                    dragToggle = false
	                end
	            end)
	        end
		end)
		
	    Frame.InputChanged:Connect(function(input)
	        if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
	            dragInput = input
	        end
		end)
		
	    game:GetService("UserInputService").InputChanged:Connect(function(input)
	        if input == dragInput and dragToggle then
	            updateInput(input)
	        end
	    end)
	end
	
	dragify(script.Parent)
end)
