local HennesseyHub = Instance.new('ScreenGui')
HennesseyHub.Name = "HennesseyHub"
HennesseyHub.ZIndexBehavior = Enum.ZIndexBehavior.Sibling
HennesseyHub.Parent = game:GetService("Players").LocalPlayer:WaitForChild("PlayerGui")

local HennesseyHubFrame = Instance.new('Frame')
HennesseyHubFrame.Name = "HennesseyHubFrame"
HennesseyHubFrame.Position = UDim2.new(0.5,0,0.5,0)
HennesseyHubFrame.Size = UDim2.new(0,436,0,212)
HennesseyHubFrame.BackgroundColor3 = Color3.new(0.1765,0.1765,0.1765)
HennesseyHubFrame.BorderColor3 = Color3.new(0,0,0)
HennesseyHubFrame.AnchorPoint = Vector2.new(0.5,0.5)
HennesseyHubFrame.Parent = HennesseyHub

local GameText = Instance.new('TextButton')
GameText.Name = "GameText"
GameText.Position = UDim2.new(0.2648,0,0.6189,0)
GameText.Size = UDim2.new(0,200,0,50)
GameText.BackgroundColor3 = Color3.new(0.1176,0.1176,0.1176)
GameText.BorderColor3 = Color3.new(1,0.5608,0)
GameText.Text = "Text"
GameText.TextColor3 = Color3.new(1,1,1)
GameText.Font = Enum.Font.Ubuntu
GameText.TextSize = 14
GameText.TextScaled = true
GameText.Parent = HennesseyHubFrame


--!fakescript
pcall(function()
	local script = { Parent = GameText }
	function OnClicked() 
		if game.PlaceId == 9217365103 then --Test Game 1
			print("1")
		elseif game.PlaceId == 9217366438 then --Test Game 2
			print("2")
		elseif game.PlaceId == 5938036553 then --Frontlines
			loadstring(game:HttpGet'https://raw.githubusercontent.com/wally-rblx/roblox-scripts/main/frontlines.lua')()
		else --Universal
			loadstring(game:HttpGet'https://raw.githubusercontent.com/HennesseyTeam/HennesseyHub/main/Universal')()
		end
	end 
	
	script.Parent.MouseButton1Down:connect(OnClicked)
	
end)
--!end


--!fakescript
pcall(function()
	local script = { Parent = GameText }
	local gametext = "text"
	
	if game.PlaceId == 9217365103 then --Test Game 1
		gametext = "Game 1"
	elseif game.PlaceId == 9217366438 then --Test Game 2
		gametext = "Game 2"
	elseif game.PlaceId == 5938036553 then --Frontlines
		gametext = "Frontlines"
	else --Universal
		gametext = "Universal ESP + Aimbot"
	end
	
	
	script.Parent.Text = gametext
end)
--!end


--!fakescript
pcall(function()
	local script = { Parent = GameText }
	function OnClicked() 
		wait(0.1)
		script.Parent.Parent.Parent:Destroy()
	end 
	
	script.Parent.MouseButton1Down:connect(OnClicked)
end)
--!end


--!fakescript
pcall(function()
	local script = { Parent = GameText }
	function OnClicked() 
		print("Animations Coming Soon!")
	end 
	
	script.Parent.MouseButton1Down:connect(OnClicked)
end)
--!end

